{
  "name": "@kronos-integration/test-interceptor",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/module.mjs"
  },
  "description": "test support for kronos interceptors",
  "keywords": [
    "test"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "devDependencies": {
    "ava": "^5.1.1",
    "c8": "^7.12.0",
    "documentation": "^14.0.1",
    "kronos-endpoint": "^2.14.15",
    "semantic-release": "^20.0.3"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/test-interceptor"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/test-interceptor/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/test-interceptor#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-kronos-component"
    ]
  }
}
